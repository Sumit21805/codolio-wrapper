{
  "name": "codolio-apk",
  "productName": "Codolio",
  "version": "1.0.0",
  "description": "Codolio Desktop Wrapper",
  "main": "main.js",
  "scripts": {
    "dist": "electron-builder",
    "installer": "node build-installer.js",
    "package": "electron-packager . --overwrite --platform=win32 --arch=x64 --icon=\"codolio image icon.ico\" --prune=true --out=release-builds --ignore=\"^/release-builds$\" --ignore=\"^/dist$\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Codolio Wrapper",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "electron": "^40.4.1",
    "electron-builder": "^26.7.0",
    "electron-packager": "^17.1.2",
    "electron-winstaller": "^5.4.0"
  },
  "build": {
    "appId": "com.codolio.wrapper",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "codolio image icon.svg"
    }
  }
}